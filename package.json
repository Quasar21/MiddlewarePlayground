  "bugs": {
    "url": "https://github.com/Lingvokot/Tic-Tac-Toe-React/issues"
  },
  "engines": {
    "node": ">= 4.0"
  },
  "homepage": "https://github.com/Lingvokot/Tic-Tac-Toe-React#readme",
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
